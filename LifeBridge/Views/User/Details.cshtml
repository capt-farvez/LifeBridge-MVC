@{
    ViewData["Title"] = "User Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h2>@Model.Name</h2>
    <p>Email: @Model.Email</p>
    <p>Phone Number: @Model.PhoneNumber</p>
    <p>Address: @Model.Address</p>
    <p>Blood Group: @Model.BloodGroup</p>
</div>
